using Nemerle.Collections;
using Nemerle.Text;
using Nemerle.Utility;

using System;
using System.Collections.Generic;
using System.Console;
using System.Linq;
using PetriNetLib;

module Program
{
  Main() : void
  {
    WriteLine("Hi!");
    def parser = Parser();
    parser.Parse("DiningPhilosophers.cpn");
    def net = PetriNet(parser.Places, parser.Transitions, parser.Arcs, parser.Decls);
    net.Process();
    _ = ReadLine();
  }
}